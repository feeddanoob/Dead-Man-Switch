  <RecipeDef>
    <defName>DMS_Make_Polyester</defName>
    <label>make polyester x50</label>
    <description>Synthetic polyester fibers using chemfuel.</description>
    <jobString>Synthesising.</jobString>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_Machining</soundWorking>
    <unfinishedThingDef>UnfinishedComponent</unfinishedThingDef>
    <recipeUsers>
      <li>DMS_ReactionChamber</li>
    </recipeUsers>
    <skillRequirements>
      <Crafting>2</Crafting>
    </skillRequirements>
    <workSkill>Crafting</workSkill>
    <researchPrerequisite>DMS_Plasfiber</researchPrerequisite>
    <workAmount>3000</workAmount>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Chemfuel</li>
          </thingDefs>
        </filter>
        <count>25</count>
